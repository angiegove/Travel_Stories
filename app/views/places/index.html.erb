  <div class= "col-sm-10 col-md-10">
    <div class= "places">
      <h1>My Places</h1>
      <% @places.each do |place| %>
        <div class="col-sm-4 col-md-4">
          <div class="thumbnail">
            <div class="caption">
              <h2><%= place.title %></h2>
            </div>
          </div>
         </div>
       <% end %>
    </div>
  </div>